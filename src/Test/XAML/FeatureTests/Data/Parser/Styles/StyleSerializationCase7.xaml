




<cmn:CustomCanvas xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:cc="http://XamlTestTypes" xmlns:cmn="clr-namespace:Microsoft.Test.Serialization.CustomElements;assembly=TestRuntime" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:S="clr-namespace:System;assembly=Mscorlib" Background="White">
        <cmn:CustomCanvas.Resources>
        <!-- 7 in style2 matrix-->
            <S:String x:Key="myString">This is a string</S:String>
            <S:String x:Key="myString1">This is another string 1</S:String>
            <Style x:Key="{x:Type Button}" TargetType="{x:Type Button}">
                <Setter Property="Content" Value="{DynamicResource myString}" />
                <Setter Property="Background" Value="White" />
                <Setter Property="Foreground" Value="Black" />
                <Setter Property="FontSize" Value="20" />              
                <Setter Property="Template">
                
                <ControlTemplate TargetType="{x:Type Button}">
                    <TextBlock x:Name="myText" Text="{DynamicResource myString1}" />
                </ControlTemplate>
                
                </Setter>
            </Style> 
        </cmn:CustomCanvas.Resources>
    <Button Name="Button2" />

</cmn:CustomCanvas>

