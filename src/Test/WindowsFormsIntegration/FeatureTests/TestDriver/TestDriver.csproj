<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AssemblyName>TestDriver</AssemblyName>
        <OutputType>EXE</OutputType>
        <SignAssembly>false</SignAssembly>
        <AssemblyOriginatorKeyFile>$(TestTrustedKey)</AssemblyOriginatorKeyFile>
        <AssemblyAttributeClsCompliant>false</AssemblyAttributeClsCompliant>
        <!-- Auto-converted: BINPLACE_FLAGS -->
        <BinplaceFlags>$(BinplaceFlags)</BinplaceFlags>
    </PropertyGroup>
    <ItemGroup>
        <Compile Include="TestDriver.cs" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="$(InternalUtilitiesProject)" />
        <ProjectReference Include="$(TestContractsProject)" />
        <ProjectReference Include="$(TestRuntimeProject)" />
    </ItemGroup>
</Project>