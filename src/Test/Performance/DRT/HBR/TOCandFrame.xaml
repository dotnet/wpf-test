        <Style x:Key="ShowHideTOCButton" TargetType="{x:Type Button}">
            <Setter Property="Cursor" Value="Hand" />
            <Setter Property="Margin" Value="7" />
            <Setter Property="Template">
                
                    <ControlTemplate TargetType="{x:Type Button}">
                        <DockPanel>
                            <Border Width="24" Height="24" Background="{TemplateBinding  Background}" />
                            <ContentPresenter Content="{TemplateBinding  Content}" />
                        </DockPanel>
                    </ControlTemplate>
                
            </Setter>
        </Style>
        <Style x:Key="ScrollingListBox" TargetType="{x:Type ListBox}">
            <Setter Property="Template">
                
                    <ControlTemplate TargetType="{x:Type ListBox}">
                        <Border Height="{TemplateBinding Height}" Width="{TemplateBinding Width}" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}">
                            <!--<ScrollViewer Height="*Alias(Target=Height)" Width="*Alias(Target=Width)">-->
                            <StackPanel Margin="20" IsItemsHost="true" />
                            <!--</ScrollViewer>-->
                        </Border>
                    </ControlTemplate>
                
            </Setter>
        </Style>
        <DataTemplate x:Key="MyDataTemplate">
            <StackPanel Height="60">
                <Image HorizontalAlignment="Left" Height="40" Source="DrtFiles\HBROpt\articon2.jpg" />
                <TextBlock HorizontalAlignment="Left" Foreground="white">
                    <TextBlock.Text>
                        <Binding Path="Filename" />
                    </TextBlock.Text>
                </TextBlock>
            </StackPanel>
        </DataTemplate>
    </Grid.Resources>
